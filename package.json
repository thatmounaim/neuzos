{
  "name": "neuzos",
  "productName": "NeuzOS",
  "version": "2.1.6",
  "description": "Launcher for Flyff Universe",
  "main": "./out/main/index.js",
  "author": {
    "name": "Abdelmounaim Bouakouk",
    "email": "abdelmounaim@bouakouk.com"
  },
  "homepage": "https://github.com/thatmounaim/neuzos",
  "scripts": {
    "format": "prettier --plugin prettier-plugin-svelte --write .",
    "lint": "eslint --cache .",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "svelte-check": "svelte-check --tsconfig ./tsconfig.json",
    "typecheck": "npm run typecheck:node && npm run svelte-check",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "dev:session": "electron-vite dev -- --mode=session --session_id=1762712661214",
    "dev:focus": "electron-vite dev -- --mode=focus --session_id=1762712661214",
    "dev:focus_fs": "electron-vite dev -- --mode=focus_fullscreen --session_id=1762712661214",
    "dev:session_launcher": "electron-vite dev -- --mode=session_launcher",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:mac": "npm run build && electron-builder --mac --x64 --arm64 --publish never",
    "build:win": "npm run build && electron-builder --win --publish never",
    "build:linux": "npm run build && electron-builder --linux --publish never",
    "release:mac": "npm run build && electron-builder --mac --x64 --arm64 --publish always",
    "release:win": "npm run build && electron-builder --win --publish always",
    "release:linux": "npm run build && electron-builder --linux --publish always"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.2",
    "@electron-toolkit/utils": "^4.0.0",
    "@lucide/svelte": "^0.544.0",
    "clsx": "^2.1.1",
    "rimraf": "^6.1.2",
    "tailwind-merge": "^3.4.0",
    "tailwind-variants": "^3.2.2",
    "tw-animate-css": "^1.4.0"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^3.0.0",
    "@electron-toolkit/eslint-config-ts": "^3.1.0",
    "@electron-toolkit/tsconfig": "^2.0.0",
    "@internationalized/date": "^3.10.1",
    "@sveltejs/vite-plugin-svelte": "^6.2.3",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/table-core": "^8.21.3",
    "@types/node": "^22.19.3",
    "bits-ui": "^2.15.4",
    "electron": "^38.7.2",
    "electron-builder": "^25.1.8",
    "electron-vite": "^4.0.1",
    "embla-carousel-svelte": "^8.6.0",
    "eslint": "^9.39.2",
    "eslint-plugin-svelte": "^3.14.0",
    "formsnap": "^2.0.1",
    "layerchart": "2.0.0-next.27",
    "mode-watcher": "^1.1.0",
    "paneforge": "^1.0.2",
    "prettier": "^3.7.4",
    "prettier-plugin-svelte": "^3.4.1",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "svelte": "^5.46.1",
    "svelte-check": "^4.3.5",
    "svelte-sonner": "^1.0.7",
    "sveltekit-superforms": "^2.29.1",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vaul-svelte": "^1.0.0-next.7",
    "vite": "^7.3.1"
  }
}
